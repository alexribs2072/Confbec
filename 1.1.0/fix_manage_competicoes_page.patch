+import React, { useEffect, useMemo, useState } from 'react';
+import axios from 'axios';
+
+import {
+  Alert,
+  Avatar,
+  Box,
+  Button,
+  Chip,
+  Container,
+  Dialog,
+  DialogActions,
+  DialogContent,
+  DialogTitle,
+  FormControl,
+  Grid,
+  InputLabel,
+  MenuItem,
+  Paper,
+  Select,
+  Table,
+  TableBody,
+  TableCell,
+  TableContainer,
+  TableHead,
+  TableRow,
+  TextField,
+  Typography,
+  Zoom,
+  CircularProgress,
+} from '@mui/material';
+
+import EmojiEventsIcon from '@mui/icons-material/EmojiEvents';
+import AddCircleOutlineIcon from '@mui/icons-material/AddCircleOutline';
+import EditIcon from '@mui/icons-material/Edit';
+import CancelIcon from '@mui/icons-material/Cancel';
+import RefreshIcon from '@mui/icons-material/Refresh';
+
+const STATUS_OPTIONS = [
+  { value: 'RASCUNHO', label: 'Rascunho' },
+  { value: 'INSCRICOES_ABERTAS', label: 'Inscrições Abertas' },
+  { value: 'INSCRICOES_ENCERRADAS', label: 'Inscrições Encerradas' },
+  { value: 'EM_ANDAMENTO', label: 'Em Andamento' },
+  { value: 'FINALIZADO', label: 'Finalizado' },
+  { value: 'CANCELADO', label: 'Cancelado' },
+];
+
+const ESCOPO_OPTIONS = [
+  { value: 'MUNICIPAL', label: 'Municipal' },
+  { value: 'ESTADUAL', label: 'Estadual' },
+  { value: 'NACIONAL', label: 'Nacional' },
+  { value: 'INTERNACIONAL', label: 'Internacional' },
+];
+
+function toDateInputValue(value) {
+  if (!value) return '';
+  try {
+    // Pode vir como ISO string (2026-02-16T00:00:00.000Z) ou Date
+    const d = typeof value === 'string' ? new Date(value) : value;
+    if (Number.isNaN(d.getTime())) return '';
+    const yyyy = d.getFullYear();
+    const mm = String(d.getMonth() + 1).padStart(2, '0');
+    const dd = String(d.getDate()).padStart(2, '0');
+    return `${yyyy}-${mm}-${dd}`;
+  } catch {
+    return '';
+  }
+}
+
+function moneyToNumber(value) {
+  if (value === null || value === undefined || value === '') return 0;
+  const n = Number(String(value).replace(',', '.'));
+  return Number.isFinite(n) ? n : 0;
+}
+
+export default function ManageCompeticoesPage() {
+  const [eventos, setEventos] = useState([]);
+  const [loading, setLoading] = useState(true);
+  const [saving, setSaving] = useState(false);
+  const [error, setError] = useState(null);
+  const [successMsg, setSuccessMsg] = useState(null);
+
+  const [query, setQuery] = useState('');
+
+  const [open, setOpen] = useState(false);
+  const [editing, setEditing] = useState(null);
+
+  const [form, setForm] = useState({
+    nome: '',
+    descricao: '',
+    local: '',
+    escopo: 'MUNICIPAL',
+    data_evento: '',
+    data_fim: '',
+    status: 'RASCUNHO',
+    taxa_inscricao: 0,
+  });
+
+  const filtered = useMemo(() => {
+    const q = query.trim().toLowerCase();
+    if (!q) return eventos;
+    return eventos.filter((e) =>
+      [e.nome, e.local, e.escopo, e.status].filter(Boolean).some((v) => String(v).toLowerCase().includes(q))
+    );
+  }, [eventos, query]);
+
+  const fetchEventos = async () => {
+    try {
+      setLoading(true);
+      const res = await axios.get('/api/competicoes/eventos');
+      setEventos(Array.isArray(res.data) ? res.data : []);
+      setError(null);
+    } catch (err) {
+      console.error(err);
+      setError(err.response?.data?.msg || 'Erro ao carregar as competições.');
+    } finally {
+      setLoading(false);
+    }
+  };
+
+  useEffect(() => {
+    fetchEventos();
+  }, []);
+
+  const resetForm = () => {
+    setForm({
+      nome: '',
+      descricao: '',
+      local: '',
+      escopo: 'MUNICIPAL',
+      data_evento: '',
+      data_fim: '',
+      status: 'RASCUNHO',
+      taxa_inscricao: 0,
+    });
+    setEditing(null);
+  };
+
+  const openCreate = () => {
+    resetForm();
+    setOpen(true);
+  };
+
+  const openEdit = (evt) => {
+    setEditing(evt);
+    setForm({
+      nome: evt.nome || '',
+      descricao: evt.descricao || '',
+      local: evt.local || '',
+      escopo: evt.escopo || 'MUNICIPAL',
+      data_evento: toDateInputValue(evt.data_evento),
+      data_fim: toDateInputValue(evt.data_fim),
+      status: evt.status || 'RASCUNHO',
+      taxa_inscricao: moneyToNumber(evt.taxa_inscricao),
+    });
+    setOpen(true);
+  };
+
+  const closeDialog = () => {
+    setOpen(false);
+    setTimeout(() => resetForm(), 200);
+  };
+
+  const handleSave = async () => {
+    setError(null);
+    setSuccessMsg(null);
+
+    if (!form.nome.trim()) {
+      setError('Informe o nome da competição.');
+      return;
+    }
+    if (!form.data_evento) {
+      setError('Informe a data do evento.');
+      return;
+    }
+
+    const payload = {
+      nome: form.nome.trim(),
+      descricao: form.descricao?.trim() || null,
+      local: form.local?.trim() || null,
+      escopo: form.escopo,
+      data_evento: form.data_evento,
+      data_fim: form.data_fim || null,
+      status: form.status,
+      taxa_inscricao: moneyToNumber(form.taxa_inscricao),
+    };
+
+    try {
+      setSaving(true);
+      if (editing?.id) {
+        await axios.put(`/api/competicoes/eventos/${editing.id}`, payload);
+        setSuccessMsg('Competição atualizada com sucesso!');
+      } else {
+        await axios.post('/api/competicoes/eventos', payload);
+        setSuccessMsg('Competição criada com sucesso!');
+      }
+      closeDialog();
+      await fetchEventos();
+      setTimeout(() => setSuccessMsg(null), 3000);
+    } catch (err) {
+      console.error(err);
+      setError(err.response?.data?.msg || 'Erro ao salvar competição.');
+    } finally {
+      setSaving(false);
+    }
+  };
+
+  const handleCancel = async (evt) => {
+    if (!window.confirm(`Cancelar a competição "${evt.nome}"?`)) return;
+    try {
+      await axios.delete(`/api/competicoes/eventos/${evt.id}`);
+      setSuccessMsg('Competição cancelada.');
+      await fetchEventos();
+      setTimeout(() => setSuccessMsg(null), 3000);
+    } catch (err) {
+      console.error(err);
+      alert(err.response?.data?.msg || 'Erro ao cancelar competição.');
+    }
+  };
+
+  const statusChip = (status) => {
+    const color =
+      status === 'INSCRICOES_ABERTAS'
+        ? 'success'
+        : status === 'INSCRICOES_ENCERRADAS'
+          ? 'warning'
+          : status === 'CANCELADO'
+            ? 'default'
+            : status === 'EM_ANDAMENTO'
+              ? 'info'
+              : status === 'FINALIZADO'
+                ? 'secondary'
+                : 'default';
+
+    return <Chip size="small" label={status} color={color} variant={status === 'CANCELADO' ? 'outlined' : 'filled'} />;
+  };
+
+  return (
+    <Container maxWidth="lg">
+      {/* Cabeçalho */}
+      <Box sx={{ mb: 4, display: 'flex', alignItems: 'center', gap: 2 }}>
+        <Avatar sx={{ bgcolor: 'primary.main', width: 56, height: 56, boxShadow: 3 }}>
+          <EmojiEventsIcon fontSize="large" />
+        </Avatar>
+        <Box sx={{ flex: 1 }}>
+          <Typography variant="h4" fontWeight="bold" color="primary.main">
+            Competições
+          </Typography>
+          <Typography variant="body1" color="text.secondary">
+            Cadastre e gerencie eventos de competição.
+          </Typography>
+        </Box>
+        <Box sx={{ display: 'flex', gap: 1 }}>
+          <Button startIcon={<RefreshIcon />} variant="outlined" onClick={fetchEventos}>
+            Atualizar
+          </Button>
+          <Button startIcon={<AddCircleOutlineIcon />} variant="contained" onClick={openCreate}>
+            Nova
+          </Button>
+        </Box>
+      </Box>
+
+      {/* Feedback */}
+      <Box sx={{ mb: 2, minHeight: 56 }}>
+        {successMsg && (
+          <Zoom in>
+            <Alert severity="success" onClose={() => setSuccessMsg(null)}>
+              {successMsg}
+            </Alert>
+          </Zoom>
+        )}
+        {error && <Alert severity="error">{error}</Alert>}
+      </Box>
+
+      {/* Busca */}
+      <Paper elevation={2} sx={{ p: 2, mb: 3, borderRadius: 3 }}>
+        <Grid container spacing={2} alignItems="center">
+          <Grid item xs={12} md={8}>
+            <TextField
+              fullWidth
+              label="Buscar (nome, local, status, escopo)"
+              value={query}
+              onChange={(e) => setQuery(e.target.value)}
+            />
+          </Grid>
+        </Grid>
+      </Paper>
+
+      {/* Lista */}
+      <Paper elevation={3} sx={{ borderRadius: 3, overflow: 'hidden' }}>
+        {loading ? (
+          <Box sx={{ p: 4, display: 'flex', justifyContent: 'center' }}>
+            <CircularProgress />
+          </Box>
+        ) : (
+          <TableContainer>
+            <Table>
+              <TableHead sx={{ bgcolor: '#f5f5f5' }}>
+                <TableRow>
+                  <TableCell><b>Nome</b></TableCell>
+                  <TableCell><b>Local</b></TableCell>
+                  <TableCell><b>Data</b></TableCell>
+                  <TableCell><b>Escopo</b></TableCell>
+                  <TableCell><b>Status</b></TableCell>
+                  <TableCell align="right"><b>Ações</b></TableCell>
+                </TableRow>
+              </TableHead>
+              <TableBody>
+                {filtered.length === 0 ? (
+                  <TableRow>
+                    <TableCell colSpan={6} align="center" sx={{ py: 4, color: 'text.secondary' }}>
+                      Nenhuma competição encontrada.
+                    </TableCell>
+                  </TableRow>
+                ) : (
+                  filtered.map((evt) => (
+                    <TableRow key={evt.id} hover>
+                      <TableCell>{evt.nome}</TableCell>
+                      <TableCell>{evt.local || '-'}</TableCell>
+                      <TableCell>
+                        {toDateInputValue(evt.data_evento) || '-'}
+                        {evt.data_fim ? ` → ${toDateInputValue(evt.data_fim)}` : ''}
+                      </TableCell>
+                      <TableCell>{evt.escopo}</TableCell>
+                      <TableCell>{statusChip(evt.status)}</TableCell>
+                      <TableCell align="right">
+                        <Button
+                          size="small"
+                          startIcon={<EditIcon />}
+                          onClick={() => openEdit(evt)}
+                          sx={{ mr: 1 }}
+                        >
+                          Editar
+                        </Button>
+                        <Button
+                          size="small"
+                          color="inherit"
+                          startIcon={<CancelIcon />}
+                          disabled={evt.status === 'CANCELADO'}
+                          onClick={() => handleCancel(evt)}
+                        >
+                          Cancelar
+                        </Button>
+                      </TableCell>
+                    </TableRow>
+                  ))
+                )}
+              </TableBody>
+            </Table>
+          </TableContainer>
+        )}
+      </Paper>
+
+      {/* Dialog */}
+      <Dialog open={open} onClose={closeDialog} fullWidth maxWidth="md">
+        <DialogTitle>
+          {editing ? 'Editar Competição' : 'Nova Competição'}
+        </DialogTitle>
+        <DialogContent dividers>
+          <Grid container spacing={2}>
+            <Grid item xs={12} md={8}>
+              <TextField
+                fullWidth
+                label="Nome"
+                value={form.nome}
+                onChange={(e) => setForm((p) => ({ ...p, nome: e.target.value }))}
+              />
+            </Grid>
+            <Grid item xs={12} md={4}>
+              <TextField
+                fullWidth
+                label="Taxa de inscrição"
+                type="number"
+                inputProps={{ step: '0.01', min: '0' }}
+                value={form.taxa_inscricao}
+                onChange={(e) => setForm((p) => ({ ...p, taxa_inscricao: e.target.value }))}
+              />
+            </Grid>
+
+            <Grid item xs={12} md={6}>
+              <TextField
+                fullWidth
+                label="Local"
+                value={form.local}
+                onChange={(e) => setForm((p) => ({ ...p, local: e.target.value }))}
+              />
+            </Grid>
+
+            <Grid item xs={12} md={3}>
+              <TextField
+                fullWidth
+                label="Data do evento"
+                type="date"
+                InputLabelProps={{ shrink: true }}
+                value={form.data_evento}
+                onChange={(e) => setForm((p) => ({ ...p, data_evento: e.target.value }))}
+              />
+            </Grid>
+
+            <Grid item xs={12} md={3}>
+              <TextField
+                fullWidth
+                label="Data fim"
+                type="date"
+                InputLabelProps={{ shrink: true }}
+                value={form.data_fim}
+                onChange={(e) => setForm((p) => ({ ...p, data_fim: e.target.value }))}
+              />
+            </Grid>
+
+            <Grid item xs={12} md={4}>
+              <FormControl fullWidth>
+                <InputLabel id="escopo-label">Escopo</InputLabel>
+                <Select
+                  labelId="escopo-label"
+                  label="Escopo"
+                  value={form.escopo}
+                  onChange={(e) => setForm((p) => ({ ...p, escopo: e.target.value }))}
+                >
+                  {ESCOPO_OPTIONS.map((o) => (
+                    <MenuItem key={o.value} value={o.value}>{o.label}</MenuItem>
+                  ))}
+                </Select>
+              </FormControl>
+            </Grid>
+
+            <Grid item xs={12} md={4}>
+              <FormControl fullWidth>
+                <InputLabel id="status-label">Status</InputLabel>
+                <Select
+                  labelId="status-label"
+                  label="Status"
+                  value={form.status}
+                  onChange={(e) => setForm((p) => ({ ...p, status: e.target.value }))}
+                >
+                  {STATUS_OPTIONS.map((o) => (
+                    <MenuItem key={o.value} value={o.value}>{o.label}</MenuItem>
+                  ))}
+                </Select>
+              </FormControl>
+            </Grid>
+
+            <Grid item xs={12}>
+              <TextField
+                fullWidth
+                label="Descrição"
+                multiline
+                minRows={3}
+                value={form.descricao}
+                onChange={(e) => setForm((p) => ({ ...p, descricao: e.target.value }))}
+              />
+            </Grid>
+          </Grid>
+        </DialogContent>
+        <DialogActions>
+          <Button onClick={closeDialog} disabled={saving}>
+            Fechar
+          </Button>
+          <Button onClick={handleSave} variant="contained" disabled={saving}>
+            {saving ? 'Salvando…' : 'Salvar'}
+          </Button>
+        </DialogActions>
+      </Dialog>
+    </Container>
+  );
+}
